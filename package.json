{
    "name": "@duckduckgo/privacy-dashboard",
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    },
    "scripts": {
        "build": "grunt build",
        "build.watch": "grunt dev --watch",
        "docs": "typedoc",
        "docs.watch": "npm run docs -- --watch",
        "preview": "npm run build && http-server build/example -o html/popup.html?state=02 -c-1",
        "serve": "http-server -c-1 --port 3210 ./",
        "lint": "eslint .",
        "lint.fix": "eslint . --fix",
        "prettier": "prettier .",
        "prettier.fix": "npm run prettier -- --write",
        "prettier.check": "npm run prettier -- --check",
        "schema": "node scripts/schema.js",
        "tsc": "tsc",
        "tsc:watch": "tsc --watch",
        "test": "npm run test.unit && npm run test.e2e",
        "test.unit": "jest",
        "test.serve": "http-server -c-1 --port 8080 ./build/example",
        "test.e2e": "npm run build && start-server-and-test 'npm run test.serve' 8080 'jest -c jest.e2e.config.js --runInBand'",
        "test.e2e.update": "npm run build && start-server-and-test 'npm run test.serve' 8080 'jest -c jest.e2e.config.js --runInBand --updateSnapshot'",
        "test.int": "playwright test",
        "test.int.headed": "playwright test --headed",
        "test.int-debug": "playwright test --headed --debug",
        "verify.local": "npm run lint.fix && npm run prettier.fix && npm run build && npm run docs && npm run test.int"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.3",
        "@babel/core": "^7.13.15",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "@material/ripple": "^14.0.0",
        "@material/switch": "^14.0.0",
        "@playwright/test": "^1.26.0",
        "@types/chrome": "^0.0.195",
        "@types/jest": "^27.5.2",
        "babelify": "10.0.0",
        "bel": "6.0.0",
        "deep-freeze": "0.0.1",
        "duckduckgo-colors": "0.0.1",
        "eslint": "^7.19.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "eventemitter2": "4.1.0",
        "grunt": "1.3.0",
        "grunt-browserify": "6.0.0",
        "grunt-contrib-copy": "1.0.0",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-exec": "3.0.0",
        "grunt-execute": "0.2.2",
        "grunt-sass": "3.1.0",
        "http-server": "^13.0.2",
        "i18next": "^21.6.14",
        "i18next-icu": "^2.0.3",
        "is-plain-object": "5.0.0",
        "jest": "^27.0.6",
        "jest-image-snapshot": "^4.5.1",
        "jest-playwright-preset": "^1.7.0",
        "jquery": "^3.6.1",
        "json-schema-to-typescript": "^11.0.2",
        "load-grunt-tasks": "3.5.2",
        "normalize.scss": "0.1.0",
        "playwright-webkit": "^1.19.1",
        "require-globify": "^1.4.1",
        "sass": "^1.32.4",
        "standard": "10.0.3",
        "start-server-and-test": "^1.14.0",
        "through2": "^2.0.5",
        "ts-to-zod": "^1.12.0",
        "typedoc": "^0.23.15",
        "typescript": "^4.8.3",
        "zod": "^3.18.0",
        "prettier": "^2.7.1"
    },
    "dependencies": {},
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "printWidth": 140,
        "tabWidth": 4
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "exclude": [
                        "transform-regenerator"
                    ],
                    "debug": false
                }
            ]
        ]
    }
}
